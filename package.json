{
  "name": "weekly-eats",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true})\"",
    "dev": "node scripts/dev-server.js",
    "dev:fast": "node scripts/dev-server.js --skip-setup",
    "dev:clean": "npm run clean && npm run dev",
    "build": "npm run setup-db && next build",
    "start": "next start",
    "lint": "next lint",
    "setup-db": "node scripts/setup-database.js",
    "postinstall": "node scripts/setup-worktree.js",
    "test": "vitest run --run --pool=forks --poolOptions.forks.singleFork=true --isolate",
    "test:watch": "vitest",
    "test:coverage": "vitest run --run --coverage --pool=forks --poolOptions.forks.singleFork=true --isolate",
    "check": "npm run lint -- --max-warnings=0 && cross-env MONGODB_URI=mongodb://localhost:27017/fake SKIP_DB_SETUP=true npm run test:coverage && cross-env MONGODB_URI=mongodb://localhost:27017/fake SKIP_DB_SETUP=true npm run build",
    "worktree:remove": "node scripts/worktree-remove.js"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.9.1",
    "@date-io/date-fns": "^3.2.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@mui/material-nextjs": "^7.3.8",
    "@mui/x-date-pickers": "^8.5.3",
    "@wei/pluralize": "npm:@jsr/wei__pluralize@^8.0.2",
    "ably": "^1.2.48",
    "convert": "^6.0.0",
    "date-fns": "^4.1.0",
    "mongodb": "^6.17.0",
    "next": "15.5.9",
    "next-auth": "^4.24.11",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.9",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "prettier": "^3.8.1",
    "typescript": "^5",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  }
}
